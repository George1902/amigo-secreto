      "source": [
        "try:\n",
        "  nombre = input('¿Cuál es el nombre del estudiante?: ')\n",
        "  resultado = notas[nombre]\n",
        "except KeyError as e:\n",
        "  print('Estudiante no matriculado(a) en el grupo')\n",
        "else:\n",
        "  print(resultado)\n",
        "finally:\n",
        "  print('La consulta ha concluído.')"
      ],
      "metadata": {
        "id": "PwdLYP_zJ04s",
        "colab": {
