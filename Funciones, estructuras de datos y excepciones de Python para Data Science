      },
      "execution_count": 14,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "¿Cuál es el nombre del estudiante?: Jorge\n",
            "[5.5, 7.5, 9.0]\n",
            "La consulta ha concluído.\n"
          ]
